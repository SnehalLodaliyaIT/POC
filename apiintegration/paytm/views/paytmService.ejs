<%_ if(!fileExists){ %>
const axios = require('axios');
const generate = require('./ChecksumGeneration');
let constants=require('../constant');
var data = {};
<%_ } %>
async function <%- methodName %>(paytmObject){
    try {
        data = JSON.stringify(paytmObject)
        Object.assign(data, {
            head: {
                "signature": await generate(data.body)
            }
        })
        var config = {
            method: <%= method %>,
            url: `${constants.PAYTM.PAYTM_BASEURL}<%- url %>`,
            headers: {
                'Content-Type': 'application/json'
            },
            data: data
        };
       let result= await axios(config)
       return result.data;
    } catch (error) {
        throw error;
    }
}
<%_ if(!fileExists){ %>
module.exports = {
    <%- methodName %>
}
<%_ } %>

var axios = require('axios');
var data = <%- JSON.stringify(details) %>;

    var config = {
    method: <%= method %>,
        url: <%= url %>,
            headers: {
            <% if(method !== 'get') { %>
                'Content-Type': <%= contentType %>,
                    <% } %>

                        'Authorization': <%= auth %>
                            },
                            data : data
                            };

                            axios(config)
                            .then(function (response) {
                            res.send(JSON.stringify(response.data))
                            })
                            .catch(function (error) {
                            res.send(JSON.stringify(error))
                            });